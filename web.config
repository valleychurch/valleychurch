<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <staticContent>
            <clientCache cacheControlMode="NoControl" cacheControlMaxAge="8.00:00:00" />
        </staticContent>
        <httpProtocol>
            <customHeaders>
                <add name="Vary" value="Accept-Encoding" />
                <add name="Access-Control-Allow-Origin" value="*" />
            </customHeaders>
        </httpProtocol>
    </system.webServer>
</configuration>
